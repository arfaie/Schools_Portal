<body class="layout layout-header-fixed" style="font-family: 'sahel';font-size:small">
    @{
        <div class="layout-header">
            <div class="navbar navbar-default">
                <div class="navbar-header">

                    <span class="navbar-brand navbar-brand-center">
                        <div>پنل مدیریت</div>
                    </span>

                    <button class="navbar-toggler visible-xs-block collapsed" type="button" data-toggle="collapse" data-target="#sidenav">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="bars">
                            <span class="bar-line bar-line-1 out"></span>
                            <span class="bar-line bar-line-2 out"></span>
                            <span class="bar-line bar-line-3 out"></span>
                        </span>
                        <span class="bars bars-x">
                            <span class="bar-line bar-line-4"></span>
                            <span class="bar-line bar-line-5"></span>
                        </span>
                    </button>
                    <button class="navbar-toggler visible-xs-block collapsed" type="button" data-toggle="collapse" data-target="#navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="arrow-up"></span>
                        <span class="ellipsis ellipsis-vertical">
                            <img class="ellipsis-object" width="32" height="32" src="~/images/logo.png" alt="Menue">
                        </span>
                    </button>
                </div>
                <div class="navbar-toggleable">
                    <nav id="navbar" class="navbar-collapse collapse">
                        <button class="sidenav-toggler hidden-xs" title="Collapse sidenav ( [ )" aria-expanded="true" type="button">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="bars">
                                <span class="bar-line bar-line-1 out"></span>
                                <span class="bar-line bar-line-2 out"></span>
                                <span class="bar-line bar-line-3 out"></span>
                                <span class="bar-line bar-line-4 in"></span>
                                <span class="bar-line bar-line-5 in"></span>
                                <span class="bar-line bar-line-6 in"></span>
                            </span>
                        </button>
                        <ul class="nav navbar-nav navbar-right">

                            <li class="dropdown">
                                @*@await Component.InvokeAsync("AdminNavbarEmail")*@
                            </li>
                            @*//////////////////////////////////////////////////////*@
                            <li class="dropdown">
                                @*@await Component.InvokeAsync("AdminNavbarProduct")*@
                            </li>
                            @*//////////////////////////////////////////////////////*@
                            <li class="dropdown">
                                @*@await Component.InvokeAsync("AdminNavbarOrders")*@
                            </li>
                        </ul>

                    </nav>
                </div>
            </div>
        </div>
        <div class="layout-main">
            <div class="layout-sidebar">
                <div class="layout-sidebar-backdrop"></div>
                <div class="layout-sidebar-body">
                    <div class="custom-scrollbar">
                        <nav id="sidenav" class="sidenav-collapse collapse">
                            <ul class="sidenav">
                                <li class="sidenav-heading sideMenueHeaderColor"> مدیریت وبسایت</li>

                                <li class="sidenav-item">
                                    <a asp-area="Admin" asp-controller="AboutUs" asp-action="Index">
                                        <span class="sidenav-icon icon icon-th"></span>
                                        <span class="sidenav-label">درباره ما</span>
                                    </a>
                                </li>
                                <li class="sidenav-item">
                                    <a asp-area="Admin" asp-controller="ContactUs" asp-action="Index">
                                        <span class="sidenav-icon icon icon-th"></span>
                                        <span class="sidenav-label">ارتباط با ما</span>
                                    </a>
                                </li>
                                <li class="sidenav-item">
                                    <a asp-area="Admin" asp-controller="Gallery" asp-action="Index">
                                        <span class="sidenav-icon icon icon-th"></span>
                                        <span class="sidenav-label">گالری تصاویر</span>
                                    </a>
                                </li>
                                <li class="sidenav-item">
                                    <a asp-area="Admin" asp-controller="Services" asp-action="Index">
                                        <span class="sidenav-icon icon icon-th"></span>
                                        <span class="sidenav-label"> سرویس ها</span>
                                    </a>
                                </li>
                                <li class="sidenav-item">
                                    <a asp-area="Admin" asp-controller="Setting" asp-action="Index">
                                        <span class="sidenav-icon icon icon-th"></span>
                                        <span class="sidenav-label">تنظیمات</span>
                                    </a>
                                </li>
                                <li class="sidenav-item">
                                    <a asp-area="Admin" asp-controller="Slider" asp-action="Index">
                                        <span class="sidenav-icon icon icon-th"></span>
                                        <span class="sidenav-label">اسلایدر</span>
                                    </a>
                                </li>
                                

                                

                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
            <div class="layout-content">

                @RenderBody()
            </div>

            <div class="layout-footer">

                <div class="layout-footer-body">
                    <small class="version" style="font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;font-size:larger">Sahad 2020</small>
                </div>
            </div>
        </div>
    }

    <script src="~/js/jquery-3.2.1.min.js"></script>
    <script src="~/js/iziToast-master/dist/js/iziToast.js"></script>
    @*<script src="~/js/jquery-cropper.js"></script>*@
    <script src="~/js/jquery.slimscroll.min.js"></script>
    <script src="~/js/elephant.min.js"></script>
    <script src="~/DataTable/datatables.js"></script>
    <script src="~/js/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/js/application.min.js"></script>
    <script src="~/js/demo.min.js"></script>
    <script src="~/js/admin-main.js"></script>

    <script src="~/DataTable/js/dataTables.responsive.js"></script>
    <script src="~/DataTable/js/responsive.bootstrap.js"></script>
    <script src="~/DataTable/js/responsive.bootstrap4.js"></script>
    <script src="~/DataTable/js/responsive.foundation.js"></script>
    <script src="~/DataTable/js/responsive.jqueryui.js"></script>
    <script src="~/DataTable/js/responsive.semanticui.js"></script>
    <script src="~/lib/select2/select2.full.min.js"></script>

    @RenderSection("Scripts", false)

    <script>
        $(document).ready(function () {
            $('#example').dataTable({

                "sdom": '<"top"i>rt<"right"flp><"clear">',
                responsive: true,

                "bFilter": true // show search input
            });
            $("#listingData_filter").addClass("hidden"); // hidden search input

            $("#serachInput").on("input", function (e) {
                e.preventDefault();
                $('#example').DataTable().search($(this).val()).draw();
            });
        });

        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'http://www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-83990101-1', 'auto');
        ga('send', 'pageview');

    </script>

    @if (TempData["Notification"] != null)
    {
        @Html.Raw(TempData["Notification"])
    }

</body>
</html>